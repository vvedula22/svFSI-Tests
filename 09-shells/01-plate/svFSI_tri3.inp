#----------------------------------------------------------------
# Mesh data
Add mesh: msh {
   Set mesh as shell: 1
   Mesh file path:    ./mesh/tri3/mesh-complete.mesh.vtu
   Add face: left {
      Face file path: ./mesh/tri3/mesh-surfaces/x0.vtp
   }
   Add face: right {
      Face file path: ./mesh/tri3/mesh-surfaces/x1.vtp
   }
   Add face: bottom {
      Face file path: ./mesh/tri3/mesh-surfaces/y0.vtp
   }
   Add face: top {
      Face file path: ./mesh/tri3/mesh-surfaces/y1.vtp
   }
   Fiber direction: ( 0.70710678118,  0.70710678118, 0.0)
   Fiber direction: ( 0.70710678118,  -0.70710678118, 0.0)
}

#----------------------------------------------------------------
# Equations
Add equation: shell {
   Coupled: 1
   Min iterations: 3
   Max iterations: 10
   Tolerance: 1e-6

   Density: 0.0
   Mass damping: 1.0
   Elasticity modulus: 12.0
   Poisson ratio: 0.5
   Shell thickness: 1.0
   #Constitutive model: Lee-Sacks {
   #   a:    0.1
   #   a0:   3.0
   #   b1:   0.3
   #   b2:   1.0
   #   mu0:  0.5
   #}
   #Constitutive model: HO-ma {
   #   a:     1.0
   #   b:     1.0
   #   a4f:   1.0
   #   b4f:   1.0
   #   a4s:   1.0
   #   b4s:   1.0
   #   afs:   1.0
   #   bfs:   1.0
   #}
   Constitutive model: MR {
      c1:    1.0
      c2:    0.5
   }

   Output: Spatial {
      Displacement: t
      Velocity: t
      Jacobian: t
      #Strain: t
      #CG_Strain: t
      CG_Strain_Trace: t
      # change name to Cauchy_strain
      #Stress: t
   }

   LS type: GMRES
   {
      Tolerance:           1e-6
      Max iterations:      100
      Krylov space dimension: 50
   }

   Add BF: msh {
      Type: Neu
      Value: 0.04
   }

   Add BC: left {
      Type: Dir
      Value: 0.0
      CST shell BC type: Fixed
   }

   Add BC: right {
      Type: Dir
      Value: 0.0
      CST shell BC type: Fixed
   }

   Add BC: bottom {
      Type: Dir
      Value: 0.0
      CST shell BC type: Fixed
   }

   Add BC: top {
      Type: Dir
      Value: 0.0
      CST shell BC type: Fixed
   }
}

